{
  "name": "site",
  "main": "electron/launcher-main.js",
  "private": true,
  "version": "0.1.7",
  "workspaces": [
    "apps/agent",
    "apps/vento",
    "apps/mcp",
    "packages/app",
    "packages/config",
    "packages/protobase",
    "packages/protodevice",
    "packages/protonode",
    "extensions/*",
    "scripts",
    "data/automations"
  ],
  "scripts": {
    "dev-service": "cross-env FULL_DEV=1 yarn prepare-dev && cross-env FULL_DEV=1 yarn start-service-fast",
    "dev": "yarn prepare-dev && yarn dev-fast",
    "dev-fast": "cross-env FULL_DEV=1 yarn start-fast",
    "fulldev": "yarn enable-ui-dev && cross-env UI_DEV=1 yarn dev",
    "fulldev-fast": "cross-env UI_DEV=1 yarn dev-fast",
    "dev-profiler": "cross-env PROFILER_ENABLED=true yarn dev",
    "dev-profiler-fast": "cross-env PROFILER_ENABLED=true yarn dev-fast",
    "start": "yarn prepare-dev && yarn start-fast",
    "start-fast": "node vento.js",
    "prepare-dev": "node vento.js --prepare && yarn build && yarn workspaces foreach --all run prepare-dev",
    "prepare-dev-ci": "node vento.js --prepare --skip-pages-download --skip-download-client && yarn build && yarn workspaces foreach --all run prepare-dev",
    "prepare": "node vento.js --prepare && yarn workspaces foreach --all run prepare-dev",
    "add-user": "node vento.js add-user",
    "build": "yarn workspaces foreach -t --all run build",
    "package": "yarn prepare-dev && yarn workspaces foreach --all run package",
    "status": "node vento.js status",
    "restart": "node vento.js restart",
    "monit": "node vento.js monit",
    "kill": "node vento.js stop",
    "stop": "node vento.js stop",
    "logs": "node vento.js logs",
    "extension": "node scripts/extension.js",
    "fix": "manypkg fix",
    "check-deps": "check-dependency-version-consistency .",
    "shrink": "node scripts/removeDevMode.js && node scripts/shrink.js",
    "vento-update": "node scripts/vento-update.js",
    "python": "node apps/python/run.js",
    "git-origin-setup": "node scripts/git-origin-setup.js",
    "launcher-dev": "cross-env SKIP_INSTALL=true electron electron/launcher-main.js --ui-dev -v",
    "launcher": "electron electron/launcher-main.js",
    "app-dev": "cross-env SKIP_INSTALL=true electron electron/dev-main.js --dev -v",
    "enable-ui-dev": "node scripts/enableUiDev.js && yarn",
    "disable-ui-dev": "node scripts/disableUiDev.js",
    "enable-launcher-dev": "node scripts/enableLauncherDev.js && yarn",
    "disable-launcher-dev": "node scripts/disableLauncherDev.js && yarn",
    "launcher-package-win": "yarn enable-launcher-dev && yarn workspace launcher package-launcher && yarn launcher-build-win",
    "launcher-build-win": "electron-builder -c electron-builder.yml --win --x64",
    "launcher-package-mac": "yarn enable-launcher-dev && yarn workspace launcher package-launcher && yarn launcher-build-mac",
    "launcher-build-mac": "electron-builder -c electron-builder.yml --mac --arm64",
    "launcher-notarize-mac": "node scripts/launcher-notarize-mac.js",
    "launcher-package-linux": "yarn enable-launcher-dev && yarn workspace launcher package-launcher && yarn launcher-build-linux",
    "launcher-build-linux": "electron-builder -c electron-builder.yml --linux --x64",
    "launcher-package-rpi": "yarn enable-launcher-dev && yarn workspace launcher package-launcher && yarn launcher-build-rpi",
    "launcher-build-rpi": "electron-builder -c electron-builder.yml --linux --arm64",
    "launcher-package-all": "yarn enable-launcher-dev && yarn workspace launcher package-launcher && electron-builder -c electron-builder.yml --win --mac --linux",
    "download-binaries": "node scripts/download-bins.js",
    "standalone-extract": "node scripts/extract.js ./vento-extracted",
    "standalone-dev": "electron electron/standalone-main.js -v",
    "standalone-build-win": "node scripts/standalone-build.js win",
    "standalone-build-mac": "node scripts/standalone-build.js mac",
    "standalone-build-linux": "node scripts/standalone-build.js linux",
    "clean-deps": "shx rm -rf node_modules",
    "update-ui": "shx rm -rf data/pages && node vento.js --prepare",
    "update-android-client": "shx rm -rf data/public/clients/vento-client.apk && node vento.js --prepare",
    "assets": "node extensions/assets/scripts/index.js",
    "vento": "node apps/cli/bin/vento.js",
    "download-agent": "node scripts/download-agent.js",
    "update-agent": "node scripts/download-agent.js --force",
    "build-agent": "node scripts/build-agent.js",
    "setup-agent": "node apps/agent/setup.js",
    "update": "node scripts/update.js && yarn update-ui && yarn update-android-client && git fetch origin --tags --force && git pull",
    "clean": "yarn workspaces foreach --all run clean && node scripts/clean.js",
    "clean:all": "yarn clean && yarn clean-deps",
    "test": "node --require ./tests/index.js --test tests/suites/*.test.js",
    "vento-agent": "claude",
    "vento-agent-send": "claude --print --dangerously-skip-permissions"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refresh": "^0.14.0",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.12"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "2.1.14",
    "adm-zip": "^0.5.16",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "minimist": "1.2.8",
    "node-gyp": "^10.1.0",
    "rimraf": "2.6.3",
    "semver": "^7.5.4",
    "tar": "7.4.3",
    "tree-kill": "^1.2.2",
    "typescript": "5.8.2",
    "uuid": "9.0.0"
  },
  "packageManager": "yarn@3.5.1",
  "devDependencies": {
    "@babel/runtime": "^7.24.6",
    "@manypkg/cli": "^0.21.4",
    "@types/adm-zip": "^0",
    "async": "3.2.5",
    "check-dependency-version-consistency": "^4.1.0",
    "cross-env": "^10.1.0",
    "electron": "29.4.6",
    "electron-builder": "24.13.3",
    "eslint": "^9.3.0",
    "fdir": "6.1.1",
    "glob": "^10.3.10",
    "ignore": "5.3.0",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "shx": "^0.3.4",
    "tar": "^7.4.3",
    "ts-node": "10.9.1"
  },
  "nohoist": [
    "hub/react-native",
    "hub/react-native-web",
    "hub/react-native/**",
    "hub/react-native-web/**"
  ]
}